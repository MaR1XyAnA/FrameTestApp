<Window x:Class="FrameTestApp.ViewFolder.WindowFolder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FrameTestApp.ViewFolder.WindowFolder"
        mc:Ignorable="d"
        Height="700"
        Width="1200"
        Title="ГЛАВНОЕ ОКНО"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="4*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>
        <Frame
            Grid.Row="0"
            Grid.Column="0"
            x:Name="FrameLogoMain"/>
        <Frame
            Grid.Row="0"
            Grid.Column="1"
            x:Name="FrameTitleMain"/>
        <Frame
            Grid.Row="1"
            Grid.Column="0"
            x:Name="FrameMenuMain"/>
        <Frame
            Grid.Row="1"
            Grid.Column="1"
            x:Name="FrameBodyMain"/>
    </Grid>
</Window>
